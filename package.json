{
  "name": "@dragon437619/easyflow-web-components",
  "version": "0.0.5-alpha",
  "description": "Easyflow web components",
  "main": "dist/index.cjs.js",
  "collection": "dist/collection/collection-manifest.json",
  "module": "dist/index.js",
  "types": "dist/types/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs.js",
      "types": "./dist/types/index.d.ts"
    },
    "./loader": {
      "import": "./loader/index.js",
      "require": "./loader/index.cjs.js",
      "types": "./loader/index.d.ts"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Dragon437619/easyflow-web-components.git"
  },
  "files": [
    "dist/",
    "loader/"
  ],
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "scripts": {
    "build": "stencil build",
    "start": "stencil build --dev --watch --serve",
    "generate": "stencil generate",
    "precommit": "npm run prettify && lint-staged --no-stash",
    "prettify": "prettier --write \"**/*.{js,jsx,ts,tsx,json,css,scss}\""
  },
  "dependencies": {
    "@stencil/core": "4.7.0",
    "@stencil-community/postcss": "2.2.0",
    "@stencil/sass": "3.0.12",
    "autoprefixer": "10.4.19"
  },
  "devDependencies": {
    "@types/node": "16.18.11",
    "eslint": "9.3.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-prettier": "5.1.3",
    "husky": "9.0.11",
    "lint-staged": "15.2.2",
    "postcss-flexbugs-fixes": "5.0.2",
    "prettier": "3.2.5",
    "typescript": "5.4.5"
  },
  "license": "MIT"
}
